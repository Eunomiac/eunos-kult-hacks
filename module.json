{
  "id": "eunos-kult-hacks",
  "title": "Euno's Kult Hacks",
  "description": "Personal modifications to the existing Foundry VTT system for Kult: Divinity Lost.",
  "version": "0.0.1",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "authors": [{
		"name": "Ryan West",
		"url": "https://github.com/Eunomiac",
		"email": "ryan.o.west@gmail.com",
		"discord": "Eunomiac#8172"
	}],
  "socket": true,
  "esmodules": ["greensock/esm/all.js", "eunos-kult-hacks.js"],
  "scripts": ["greensock/dist/gsap.min.js"],
  "styles": ["styles.css"],
  "packs": [
    {
      "label": "Advantages",
      "name": "advantages",
      "type": "Item",
      "system": "k4lt",
      "path": "packs/advantages"
    },
    {
      "label": "Dark Secrets",
      "name": "dark-secrets",
      "type": "Item",
      "system": "k4lt",
      "path": "packs/dark-secrets"
    },
    {
      "label": "Disadvantages",
      "name": "disadvantages",
      "type": "Item",
      "system": "k4lt",
      "path": "packs/disadvantages"
    },
    {
      "label": "Moves",
      "name": "moves",
      "type": "Item",
      "system": "k4lt",
      "path": "packs/moves"
    },
    {
      "label": "CHARACTERS",
      "name": "characters",
      "type": "Actor",
      "system": "k4lt",
      "path": "packs/characters"
    },
    {
      "label": "Custom Items",
      "name": "custom-items",
      "type": "Item",
      "system": "k4lt",
      "path": "packs/custom-items"
    },
    {
      "label": "Generic NPCs",
      "name": "generic-npcs",
      "type": "Actor",
      "system": "k4lt",
      "path": "packs/generic-npcs"
    }
  ],
  "relationships": {
    "systems": [{
      "id": "k4lt",
      "type": "system",
      "manifest": "https://github.com/YanKlInnomme/FoundryVTT-k4lt/releases/latest/download/system.json",
      "compatibility": {
        "verified": "4.0.0.1"
      }
    }],
    "requires": [{
      "id": "socketlib",
      "type": "module"
    }]
  }
}
